{
  "name": "@crud-query-parser/typeorm",
  "version": "0.0.2",
  "description": "Converts parsed requests into TypeORM queries",
  "license": "MIT",
  "author": "Guilherme Chaguri",
  "scripts": {
    "start:samples": "ts-node ./samples/main.ts",
    "build": "tsup --platform browser",
    "prepublish": "npm run build"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "target": "es2022",
    "format": [
      "esm",
      "cjs"
    ],
    "outDir": "./dist",
    "dts": true,
    "cjsInterop": true,
    "minify": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "skipNodeModulesBundle": true,
    "keepNames": true,
    "bundle": true
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "peerDependencies": {
    "@crud-query-parser/core": "*",
    "typeorm": "0.2.x || 0.3.x"
  },
  "devDependencies": {
    "@crud-query-parser/core": "*",
    "@types/node": "^17.0.23",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.1.0",
    "typeorm": "^0.3.3",
    "typescript": "^5.5.2"
  }
}
