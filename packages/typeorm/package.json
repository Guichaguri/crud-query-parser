  "devDependencies": {
    "@crud-query-parser/core": "*",
    "@types/node": "^17.0.23",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.3.3"
  }
