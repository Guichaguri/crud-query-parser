{
  "name": "@crud-query-parser/nestjs",
  "version": "0.0.1",
  "description": "NestJS helpers to parse queries from HTTP requests",
  "license": "MIT",
  "author": "Guilherme Chaguri",
  "scripts": {
    "build": "rimraf ./dist & tsc",
    "prepare": "npm run build",
    "test": "ts-node ./test/test.ts"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./parsers/crud": {
      "require": "./dist/parsers/crud/index.js",
      "types": "./dist/parsers/crud/index.d.ts"
    }
  },
  "peerDependencies": {
    "@crud-query-parser/core": "*",
    "@nestjs/core": ">=10.0.0",
    "@nestjs/swagger": ">=7.0.0"
  },
  "peerDependenciesMeta": {
    "@nestjs/swagger": {
      "optional": true
    }
  },
  "devDependencies": {
    "@crud-query-parser/core": "*",
    "@nestjs/common": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@types/node": "^17.0.23",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
